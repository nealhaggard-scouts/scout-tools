{
  "name": "plc-plan",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "projectType": "application",
  "sourceRoot": "apps/plc-plan/src",
  "prefix": "scout-tools",
  "targets": {
    "build": {
      "executor": "@angular-devkit/build-angular:browser",
      "outputs": [
        "{options.outputPath}"
      ],
      "options": {
        "outputPath": "dist/apps/plc-plan",
        "index": "apps/plc-plan/src/index.html",
        "main": "apps/plc-plan/src/main.ts",
        "polyfills": "apps/plc-plan/src/polyfills.ts",
        "tsConfig": "apps/plc-plan/tsconfig.app.json",
        "inlineStyleLanguage": "scss",
        "assets": [
          "apps/plc-plan/src/favicon.ico",
          "apps/plc-plan/src/assets"
        ],
        "styles": [
          "apps/plc-plan/src/styles.scss"
        ],
        "scripts": []
      },
      "configurations": {
        "production": {
          "budgets": [
            {
              "type": "initial",
              "maximumWarning": "3mb",
              "maximumError": "5mb"
            },
            {
              "type": "anyComponentStyle",
              "maximumWarning": "6kb",
              "maximumError": "10kb"
            }
          ],
          "fileReplacements": [
            {
              "replace": "apps/plc-plan/src/environments/environment.ts",
              "with": "apps/plc-plan/src/environments/environment.prod.ts"
            }
          ],
          "outputHashing": "all"
        },
        "development": {
          "buildOptimizer": false,
          "optimization": false,
          "vendorChunk": true,
          "extractLicenses": false,
          "sourceMap": true,
          "namedChunks": true
        }
      },
      "defaultConfiguration": "production"
    },
    "serve": {
      "executor": "@angular-devkit/build-angular:dev-server",
      "configurations": {
        "production": {
          "browserTarget": "plc-plan:build:production"
        },
        "development": {
          "browserTarget": "plc-plan:build:development"
        }
      },
      "defaultConfiguration": "development"
    },
    "extract-i18n": {
      "executor": "@angular-devkit/build-angular:extract-i18n",
      "options": {
        "browserTarget": "plc-plan:build"
      }
    },
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "options": {
        "lintFilePatterns": [
          "apps/plc-plan/**/*.ts",
          "apps/plc-plan/**/*.html"
        ]
      }
    },
    "test": {
      "executor": "@nrwl/jest:jest",
      "outputs": [
        "{workspaceRoot}/coverage/{projectRoot}"
      ],
      "options": {
        "jestConfig": "apps/plc-plan/jest.config.ts",
        "passWithNoTests": true
      }
    },
    "deploy": {
      "executor": "@angular/fire:deploy",
      "options": {
        "prerender": false,
        "ssr": false,
        "browserTarget": "plc-plan:build:production",
        "firebaseProject": "plc-plan",
        "firebaseHostingSite": "plc-plan"
      }
    }
  },
  "tags": []
}
